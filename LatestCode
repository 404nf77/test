@echo off

:updateCheck
curl https://raw.githubusercontent.com/404nf77/test/main/Version > x.ver
set /p version=<"x.ver"
del x.ver
if %version%==2 (goto home) else (goto update)

:home
cls
echo Welcome, User!
pause <nul

:update
cls
echo You need an update!
pause <nul
curl https://raw.githubusercontent.com/404nf77/test/main/LatestCode > App.bat
