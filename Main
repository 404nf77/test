@echo off
 
curl https://pastebin.com/raw/Hv30wUbK > lv.txt
cls
set /p lv=<"lv.txt"
del lv.txt
if %lv%==2 (goto main) else (goto update)
 
:main
echo Home Screen
pause >nul
goto main
:update
echo Press any key to install the latest update!
pause >nul
 
curl https://pastebin.com/raw/W3BFKkiF > app.bat
 
goto update
